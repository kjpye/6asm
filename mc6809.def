;	.nolist
; Definitions for the MC6809 processor

        .param pcwidth = 4

	.pushmem ram
	.defmem bottom = 0x0
	.defmem top = 0xffff
	.defmem width = 8
	.defmem executable = 1
	.defmem writeable = 1
	.defmem readable = 1
	.defmem init = 1
	.defmem label = 'R'
	.defmem pcwidth = 4
	.popmem
	
	.macro .ram
	.pushmem ram
	.endm
	
	.macro .dseg
	.pushmem ram
	.endm

