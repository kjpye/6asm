;        nolist
; Definitions for Extensa ISA

         .param codewidth = 10
         .para, pcwidth   =  8

; instruction definitions
 .isa extensa
; core
 .instruction rrr_rt   abs    0x600100
 .instruction rrr_rst  add    0x800000
 .instruction rri8_st  addi   0x00c002
 .instruction rri8_st8 addmi  0x00d002
 .instruction rrr_rst  addx2  0x900000
 .instruction rrr_rst  addx4  0xa00000
 .instruction rrr_rst  addx8  0xb00000
 .instruction rrr_rst  and    0x100000
 .instruction rri8_st  ball   0x004007
 .instruction rri8_b8  bany   0x008007
 .instruction rri8_b8  bbc    0x005007
 .instruction rri8_bi  bbci   0x006007
 .instruction rri8_bi  bbci.l 0x006007
 .instruction rri8_b8  bbs    0x00d007
 .instruction rri8_bi  bbsi   0x00e007
 .instruction rri8_bi  bbsi.l 0x00e007
 .instruction rri8_b8  beq    0x001007
 .instruction rri8_bii beqi   0x000026
 .instruction bri12_s  beqz   0x000016
 .instruction rri8_b8  bge    0x00a007
 .instruction rri8_bii bgei   0x0000e6
 .instruction rri8_b8  bgeu   0x00b007
 .instruction rri8_bii bgeui  0x0000f6
 .instruction bru12_s  bgez   0x0000d6
 .instruction rri8_b8  blt    0x002007
 .instruction rri8_bi  blti   0x0000a6
 .instruction rri8_b8  bltu   0x003007
 .instruction bri8_rs  bltui  0x0000b6
 .instruction bri12_s  bltz   0x000096
 .instruction rri8_st  bnall  0x00c007
 .instruction rri8_st  bne    0x009007
 .instruction bri8_rs  bnei   0x000066
 .instruction bri12_s  bnez   0x000056
 .instruction rri8_bi  bnone  0x000007
 .instruction call     call0  0x000005
 .instruction callx    callx0 0x0000c0
; debug option
 .instruction rrr_ab   break  0x004000
 .instruction rrrn_s   break.n  0xf02d ; only with code density option
; boolean option
 .instruction rrr_bst  all4   0x009000
 .instruction rrr_bst  all8   0x00b000
 .instruction rrr_brst andb   0x020000
 .instruction rrr_brst andbc  0x120000
 .instruction rrr_bst  any4   0x008000
 .instruction rrr_bst  any8   0x00a000
 .instruction rri8_b8  bf     0x000076
 .instruction rri8_b8  bt     0x001076
; floating point option
 .instruction rrr_rs   abs.s  0xfa0010
 .instruction rrr_rst  add.s  0x0a0000
 .instruction rfs_rst  ceil.s 0xda0000
; code density option
 .instruction rrrn_rst add.n    0x000a
 .instruction rrrn_rsi addi.n   0x000b
 .instruction ri6_s    beqz.n   0x008c
 .instruction ri6_s    bnez.n   0x00cc
; windowed register option
 .instruction call     call4  0x000015
 .instruction call     call8  0x000025
 .instruction call     call12 0x000035
 .instruction callx    callx4 0x0000d0
 .instruction callx    callx8 0x0000e0
 .instruction callx    callx12 0x0000f0
; miscellaneous operation option
 .instruction rri      clamps  0x330000 ; 00110011rrrrssss????0000
; data cache option
 .instruction rri8     dhi     0x007062 ; iiiiiiii0111ssss01100010
 .instruction rri8     dhwb    0x007042 ; iiiiiiii0111ssss01000010
 .instruction rri8     dhwbi   0x007052 ; iiiiiiii
; data cache index lock option
 .instruction rri4     dhu     0x027082 ; iiii00100111ssss10000010
 .instruction rri8     dhwb    0x007042 ; iiiiiiii0111ssss01000010
 .instruction rri8     dhwbi   0x007052 ; iiiiiiii0111ssss01010010
 .instruction rri8     dii     0x007072 ; iiiiiiii0111ssss01110010
 .instruction rri4     diu     0x037042 ; iiii00110111ssss10000010
 .instruction rri4     diwb    0x047042 ; iiii01000111ssss10000010
 .instruction rri4     diwbi   0x057042 ; iiii010100111ssss10000010
 .instruction rri4     dpfl    0x007042 ; iiii00000111ssss10000010
 .instruction rri8     dpfr    0x007002 ; iiiiiiii0111ssss00000010
 .instruction rri8     dpfro   0x007022 ; iiiiiiii0111ssss00100010 dpfro as, 0..1020
 .instruction rri8     dpfw    0x007012 ; iiiiiiii0111ssss00010010 dpfw as, 0..1020
 .instruction rri8     dpfwo   0x007032 ; iiiiiiii0111ssss00110010 dpfwo as, 0..1020
 .instruction rrr      dsync   0x002030 ; 000000000010000000110000 dsync
 .instruction bri12    entry   0x000036 ; iiiiiiiiiiiissss00110110 entry as, 0..32760
 .instruction rrr      esync   0x002020 ; 000000000010000000100000 342 esync
 .instruction rrr      excw    0x002080 ; 000000000010000010000000 343 excw
 .instruction rrr      extui   0x040000 ; oooo010arrrraaaatttt0000 344 extui ar, at, shitftimm, askimm
 .instruction 
