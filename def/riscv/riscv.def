	.nolist
; Definitions for the Risc-V processor

	.param codewidth = 32
        .param pcwidth = 8

	.pushmem ram
	.defmem bottom = 0x0
	.defmem top = 0xffffffff
	.defmem width = 32
	.defmem executable = 1
	.defmem writeable = 1
	.defmem readable = 1
	.defmem init = 1
	.defmem label = ''
	.defmem pcwidth = 8
        .defmem pcinc = 4
	.popmem
	
	.macro .ram
	.pushmem ram
	.endm
	
	.macro .dseg
	.pushmem ram
	.endm

        .include rv32i.def
        .include rv64i.def
        .include rv32m.def
        .include rv64m.def
        .include rv32a.def
        .include rv64a.def
        .include rv32f.def
        .include rv64f.def
        .include rv32d.def
        .include rv64d.def

 .assemble

 .arch riscv

 .list
